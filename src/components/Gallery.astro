---
interface Props {
  images: { src: string; alt: string }[];
}

const { images } = Astro.props;
---

<div class="grid gap-4 sm:grid-cols-2 lg:grid-cols-3">
  {images.map((image) => (
    <img src={image.src} alt={image.alt} class="h-56 w-full rounded-lg object-cover" loading="lazy" />
  ))}
</div>
